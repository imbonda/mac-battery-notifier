<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>bonda.battery.notifier.app</string>

    <key>Program</key>
    <string>/usr/local/bin/battery_notifier.sh</string>

    <key>StartInterval</key>
    <integer>300</integer> <!-- Runs every 5 minutes (300 seconds) -->

    <key>RunAtLoad</key>
    <true/> <!-- Optional: Runs immediately when loaded -->
</dict>
</plist>

